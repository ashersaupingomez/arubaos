{
  "name": "arubaos",
  "version": "3.0.0",
  "description": "Superagent utilities for interacting with the ArubaOS REST API",
  "main": "dist/cjs",
  "module": "dist/esm",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc --outDir dist/cjs && tsc --outDir dist/esm --module es2020",
    "docs:build": "documentation readme src --section=API --parse-extension=ts",
    "prepare": "npm run clean && npm run build && npm run docs:build",
    "test": "npm run prepare && node -r dotenv/config dist/esm/useClient.test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashersaupingomez/arubaos.git"
  },
  "author": "Asher Gomez <ashersaupingomez@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ashersaupingomez/arubaos/issues"
  },
  "homepage": "https://github.com/ashersaupingomez/arubaos#readme",
  "dependencies": {
    "superagent": "^6.1.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/baretest": "^2.0.0",
    "@types/node": "^14.14.10",
    "@types/superagent": "^4.1.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "baretest": "^2.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "typescript": "^4.1.2"
  }
}
